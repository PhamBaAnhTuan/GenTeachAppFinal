{
  "expo": {
    "name": "GenTeach",
    "slug": "GenTeach",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/background/logo2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tuanpham03.GenTeachApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/background/logo2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/background/logo2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "API_URL": "https://67dd4acae00db03c406affcf.mockapi.io/api/",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e56f4b64-d6b1-4a5a-9b03-2d7b475f2da4"
      }
    }
  }
}
